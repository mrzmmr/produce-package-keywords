{
  "name": "produce-package-keywords",
  "version": "1.1.5",
  "description": "Produces keywords for a package by analyzing its package.json, readme and/or given file's text",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --fix src",
    "tape": "tape src/test/*.js | tap-spec",
    "clean": "rimraf coverage .nyc_output coverage.lcov",
    "test": "npm run lint && nyc npm run tape",
    "posttest": "npm run coverage",
    "prepublish": "npm t && npm run clean",
    "pregit": "npm t && npm run clean",
    "git": "git commit -a -m",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/produce-package-keywords.git"
  },
  "keywords": [
    "produce-package-keywords",
    "produce-keywords",
    "package-keywords",
    "produce",
    "package",
    "keywords"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/produce-package-keywords/issues"
  },
  "homepage": "https://github.com/mrzmmr/produce-package-keywords#readme",
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "fs-exists-sync": "^0.1.0",
    "nlcst-to-string": "^2.0.0",
    "retext": "^4.0.0",
    "retext-keywords": "^3.1.0"
  }
}
